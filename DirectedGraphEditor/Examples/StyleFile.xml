<?xml version='1.0' encoding='UTF-8'?>
<DirectedGraphStyles>
  <GlobalStyles>
    <Background>#1E1E1E</Background>
  </GlobalStyles>
  <NodeStyles>
    <Style name="Default">
      <Width>120</Width>
      <Height>60</Height>
      <CornerRadius>8</CornerRadius>
      <Fill>#444</Fill>
      <BorderBrush>White</BorderBrush>
      <BorderThickness>1</BorderThickness>
      <PinRadius>6</PinRadius>
      <PinOffsetX>10</PinOffsetX>
      <PinSpacingY>20</PinSpacingY>
    </Style>
    <Style name="Warning">
      <Fill>#663300</Fill>
      <BorderBrush>Orange</BorderBrush>
    </Style>
  </NodeStyles>
  <EdgeStyles>
    <Style name="Default">
      <Stroke>Yellow</Stroke>
      <StrokeThickness>2</StrokeThickness>
      <ShowArrowhead>false</ShowArrowhead>
    </Style>
    <Style name="Emphasis">
      <Stroke>Red</Stroke>
      <StrokeThickness>3</StrokeThickness>
      <ShowArrowhead>true</ShowArrowhead>
    </Style>
  </EdgeStyles>
</DirectedGraphStyles>

